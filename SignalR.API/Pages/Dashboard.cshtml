@page
@model DashboardModel
@{
    ViewData["Title"] = "Dashboard Policy";
}
<h1>@ViewData["Title"]</h1>

<div>
    <div class="row">
        <div class="col-md-4">
            <div class="x_panel">
                <div class="x_title">
                    <h4>Products</h4>
                    <div class="clearfix"></div>
                </div>
                <div class="x_content">
                    <div>
                        <label for="myInput">Enter some text:</label>
                        <input type="text" id="myInput" name="myInput">
                        <button onclick="SaveSession()">Init Sessioin</button>

                    </div>
                    <table class="table table-hover" id="tblProduct">
                        <thead>
                            <tr>
                                <th>#</th>
                                <th>Name</th>
                                <th>Category</th>
                                <th>Price</th>
                            </tr>
                        </thead>
                        <tbody></tbody>
                    </table>
                </div>
            </div>
        </div>        
    </div>    
</div>

@section Scripts{    
    <script src="~/lib/signalr/dist/browser/signalr.js"></script>
    <script src="~/js/dashboard.js"></script>    
}